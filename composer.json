{
  "name": "kagg-design/kagg-pagespeed-optimization",
  "description": "KAGG PageSpeed Optimization",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "pagespeed",
    "pagespeed insights",
    "optimization"
  ],
  "homepage": "https://wordpress.org/kagg-pagespeed-optimization/",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "KAGG Design",
      "email": "info@kagg.eu",
      "homepage": "https://kagg.eu/en/"
    }
  ],
  "support": {
    "issues": "https://wordpress.org/support/plugin/kagg-pagespeed-optimization"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/tedious/JShrink.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "platform": {
      "php": "7.2"
    }
  },
  "require": {
    "ext-json": "*",
    "tedivm/jshrink": "^v1.7.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.11.1",
    "phpcompatibility/php-compatibility": "^9.3.5",
    "phpcompatibility/phpcompatibility-wp": "^2.1.5",
    "wp-coding-standards/wpcs": "^3.1.0"
  },
  "autoload": {
    "psr-4": {
      "KAGG\\PageSpeed\\Optimization\\": "src/php"
    }
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs --colors --standard=phpcs.xml"
  }
}
